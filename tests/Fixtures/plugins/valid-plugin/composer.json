{
    "name": "test/valid-plugin",
    "description": "A valid test plugin for TDD",
    "type": "nativephp-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Test\\ValidPlugin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Test\\ValidPlugin\\ValidPluginServiceProvider"
            ]
        },
        "nativephp": {
            "manifest": "nativephp.json"
        }
    },
    "require": {
        "php": "^8.1",
        "nativephp/mobile": "^1.0"
    }
}
